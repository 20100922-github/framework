<p id="notice"><%= notice %></p>

<p>
  <b>File:</b>
  <%= image_tag @medium.file.url(:medium) %>
</p>
<p>
  <b>Note:</b>
  <%= @medium.note %>
</p>
<p>
  <%= @medium.comments.find_each do |c| %>
