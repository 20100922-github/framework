<!DOCTYPE html>
<html>
<head>
  <title>Framework2</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
  <div class="menu">
    <%= image_tag "help_64x64.png" %>
    <span><%= link_to 'home', root_path %> || <%= link_to 'folders', folders_path %> | <%= link_to 'tags', tags_path %> | <%= link_to 'media', media_path %> | <%= link_to 'comments', comments_path %> || <%= link_to 'users', users_path %> | <%= link_to 'roles', roles_path %> | <%= link_to 'permissions', permissions_path %> | <%= render 'devise/menu/login_items' %></span>
  </div>
  <div class="table">
    <div class="tr top">
      <div class="td left">
      </div>
      <div class="td center">
        <div class="infobar">
          <p class="notice"><%= notice %></p>
          <p class="alert"><%= alert %></p>
        </div>
        <div class="content">
          <%= yield %>
        </div>
      </div>
      <div class="td right">
      </div>
    </div>
    <div class="tr middle">
      <div class="td left">
      </div>
      <div class="td center">
      </div>
      <div class="td right">
      </div>
    </div>
    <div class="tr bottom">
      <div class="td left">
      </div>
      <div class="td center">
      </div>
      <div class="td right">
      </div>
    </div>
  </div>
</body>
</html>
